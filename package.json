{
  "name": "flow",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
   "pnpm": {
    "overrides": {
      "react": "^19.1.0",
      "@types/react": "^19.1.6", 
      "react-dom": "^19.1.0",
      "@types/react-dom": "^19.1.6"
    }
  }
}
